{
  "Logging": {
    "PathFormat": "Logs/WebApiLog-{Date}.txt",
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Trace",
      "Microsoft.AspNetCore.Mvc.Internal": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
      "Spike.WebApi": "Information"
    },
    "IsJson": true,
    "FileSizeLimitBytes": 10485760,
    "RetainedFileCountLimit": 7
  },
  "AutofacConfig": {
    "RepositoryConfig": ".\\autofac.json"
  },
  "JwtIssuer": "http://localhost:53702/",
  "SpikeAudience": "MartinAPI",
  "SpikeReferenceAudience": "MartinAPIReference",
  "ScopeReferenceSecret": "YetAnotherKey"
}
